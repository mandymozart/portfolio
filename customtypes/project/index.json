{
	"id": "project",
	"label": "Project",
	"format": "custom",
	"repeatable": true,
	"status": true,
	"json": {
		"Main": {
			"uid": {
				"config": {
					"label": "slug",
					"placeholder": ""
				},
				"type": "UID"
			},
			"name": {
				"type": "Text",
				"config": {
					"label": "name",
					"placeholder": ""
				}
			},
			"description": {
				"type": "StructuredText",
				"config": {
					"label": "description",
					"placeholder": "",
					"allowTargetBlank": true,
					"single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
				}
			},
			"industry": {
				"type": "Text",
				"config": {
					"label": "industry",
					"placeholder": ""
				}
			},
			"type": {
				"type": "Text",
				"config": {
					"label": "type",
					"placeholder": ""
				}
			},
			"link": {
				"type": "Link",
				"config": {
					"label": "link",
					"placeholder": "",
					"select": null
				}
			},
			"fromDate": {
				"type": "Date",
				"config": {
					"label": "fromDate",
					"placeholder": ""
				}
			},
			"toDate": {
				"type": "Date",
				"config": {
					"label": "toDate",
					"placeholder": ""
				}
			},
			"video_id": {
				"type": "Text",
				"config": {
					"label": "Video ID",
					"placeholder": "(video id for external storage)"
				}
			},
			"transcript": {
				"type": "StructuredText",
				"config": {
					"label": "transcript",
					"placeholder": "",
					"allowTargetBlank": true,
					"single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
				}
			},
			"images": {
				"type": "Group",
				"config": {
					"label": "images",
					"fields": {
						"image": {
							"type": "Image",
							"config": {
								"label": "image",
								"constraint": {},
								"thumbnails": [
									{
										"name": "mobile",
										"width": 1080,
										"height": 1920
									},
									{
										"name": "thumbnail",
										"width": 960,
										"height": 540
									}
								]
							}
						}
					}
				}
			},
			"skills": {
				"type": "Group",
				"config": {
					"label": "Skills",
					"fields": {
						"skill": {
							"type": "Link",
							"config": {
								"label": "skill",
								"select": "document",
								"customtypes": ["skill"]
							}
						}
					}
				}
			},
			"partners": {
				"type": "Group",
				"config": {
					"label": "Partners",
					"fields": {
						"partner": {
							"type": "Link",
							"config": {
								"label": "Partner",
								"select": "document",
								"customtypes": ["client", "collaborator"]
							}
						}
					}
				}
			}
		}
	}
}
